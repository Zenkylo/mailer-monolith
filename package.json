{
  "name": "mailer",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build --ignore-ts-errors",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "lint:check": "eslint . --ext .js,.ts,.vue --max-warnings 0",
    "format": "prettier --write \"**/*.{js,ts,vue,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,vue,json,css,scss,md}\"",
    "code:fix": "npm run lint:fix && npm run format",
    "code:check": "npm run lint:check && npm run format:check",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js",
    "#jobs/*": "./app/jobs/*.js",
    "#types/*": "./app/types/*.js",
    "#services/*": "./app/services/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "7.8.2",
    "@adonisjs/eslint-config": "2.0.0",
    "@adonisjs/prettier-config": "1.4.4",
    "@adonisjs/tsconfig": "1.4.0",
    "@japa/assert": "4.0.1",
    "@japa/plugin-adonisjs": "4.0.0",
    "@japa/runner": "4.2.0",
    "@swc/core": "1.11.24",
    "@types/luxon": "3.7.1",
    "@types/node": "22.15.18",
    "@types/underscore": "1.13.0",
    "@vitejs/plugin-vue": "5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "daisyui": "5.3.7",
    "eslint": "9.26.0",
    "eslint-plugin-vue": "^10.6.2",
    "hot-hook": "0.4.0",
    "pino-pretty": "13.0.0",
    "prettier": "3.5.3",
    "prettier-plugin-vue": "^1.1.6",
    "ts-node-maintained": "10.9.5",
    "typescript": "~5.8.3",
    "vite": "6.3.5"
  },
  "dependencies": {
    "@adonisjs/auth": "9.4.0",
    "@adonisjs/bouncer": "3.1.6",
    "@adonisjs/core": "6.18.0",
    "@adonisjs/cors": "2.2.1",
    "@adonisjs/i18n": "2.2.2",
    "@adonisjs/inertia": "3.1.1",
    "@adonisjs/limiter": "2.4.0",
    "@adonisjs/lucid": "21.6.1",
    "@adonisjs/mail": "9.2.2",
    "@adonisjs/redis": "9.2.0",
    "@adonisjs/session": "7.5.1",
    "@adonisjs/shield": "8.2.0",
    "@adonisjs/static": "1.1.1",
    "@adonisjs/vite": "4.0.0",
    "@aws-sdk/client-ses": "3.876.0",
    "@inertiajs/vue3": "2.1.2",
    "@nemoventures/adonis-jobs": "1.1.0",
    "@polar-sh/sdk": "0.37.0",
    "@primeuix/themes": "1.2.3",
    "@tailwindcss/vite": "4.1.12",
    "@vinejs/vine": "3.0.1",
    "@vue-js-cron/light": "5.1.0",
    "axios": "1.11.0",
    "better-sqlite3": "12.2.0",
    "bullmq": "5.58.2",
    "cron-parser": "4.9.0",
    "edge.js": "6.2.1",
    "luxon": "3.7.1",
    "mjml": "4.15.3",
    "pg": "8.16.3",
    "primeicons": "7.0.0",
    "primevue": "4.3.7",
    "reflect-metadata": "0.2.2",
    "tailwindcss": "4.1.12",
    "underscore": "1.13.7",
    "vue": "3.5.19",
    "vue-i18n": "11.1.11",
    "vue-toastification": "2.0.0-rc.5"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config"
}
